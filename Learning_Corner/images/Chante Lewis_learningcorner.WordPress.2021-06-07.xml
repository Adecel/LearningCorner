<item>
  <title>Career Information</title>
  <link>https://learningcorner12345.wordpress.com/career-information/</link>
  <pubDate>Thu, 27 May 2021 16:31:27 +0000</pubDate>
  <dc:creator>chantelewis97</dc:creator>
  <guid isPermaLink="false">https://learningcorner12345.wordpress.com/?page_id=24</guid>
  <description/>
  <content:encoded><![CDATA[<!-- wp:cover {"url":"https:\/\/learningcorner12345.files.wordpress.com\/2021\/05\/back-24-1.png","id":70,"dimRatio":0,"overlayColor":"background","minHeight":100,"minHeightUnit":"vh","contentPosition":"center center","align":"full","className":"is-style-default"} -->
<div class="wp-block-cover alignfull has-background-background-color is-style-default" style="min-height:100vh;"><img class="wp-block-cover__image-background wp-image-70" alt="" src="https://learningcorner12345.files.wordpress.com/2021/05/back-24-1.png" data-object-fit="cover" /><div class="wp-block-cover__inner-container"><!-- wp:jetpack/layout-grid {"column1DesktopSpan":5,"column1TabletSpan":8,"column1MobileSpan":4,"column2DesktopOffset":10,"className":"column1-desktop-grid__span-5 column1-desktop-grid__row-1 column1-tablet-grid__span-8 column1-tablet-grid__row-1 column1-mobile-grid__span-4 column1-mobile-grid__row-1"} -->
<div class="wp-block-jetpack-layout-grid alignfull column1-desktop-grid__span-5 column1-desktop-grid__row-1 column1-tablet-grid__span-8 column1-tablet-grid__row-1 column1-mobile-grid__span-4 column1-mobile-grid__row-1"><!-- wp:jetpack/layout-grid-column -->
<div class="wp-block-jetpack-layout-grid-column wp-block-jetpack-layout-grid__padding-none"><!-- wp:heading {"className":"margin-bottom-none","textColor":"primary","style":{"typography":{"lineHeight":"1.65","fontSize":18}}} -->
<h2 class="margin-bottom-none has-primary-color has-text-color" style="font-size:18px;line-height:1.65;">OUR  PROMISES</h2>
<!-- /wp:heading -->

<!-- wp:paragraph {"className":"margin-top-half","textColor":"primary","style":{"typography":{"lineHeight":"1.5","fontSize":26}}} -->
<p class="margin-top-half has-primary-color has-text-color" style="font-size:26px;line-height:1.5;">We organically source all of our materials striving for the lowest footprint possible. Kind to your skin. Kind to the environment.</p>
<!-- /wp:paragraph -->

<!-- wp:buttons -->
<div class="wp-block-buttons"><!-- wp:button {"textColor":"primary","className":"is-style-outline"} -->
<div class="wp-block-button is-style-outline"><a class="wp-block-button__link has-primary-color has-text-color">Learn more</a></div>
<!-- /wp:button --></div>
<!-- /wp:buttons --></div>
<!-- /wp:jetpack/layout-grid-column --></div>
<!-- /wp:jetpack/layout-grid --></div></div>
<!-- /wp:cover -->

<!-- wp:group {"align":"full"} -->
<div class="wp-block-group alignfull"><!-- wp:spacer {"height":64} -->
<div style="height:64px;" aria-hidden="true" class="wp-block-spacer"></div>
<!-- /wp:spacer -->

<!-- wp:columns {"align":"wide"} -->
<div class="wp-block-columns alignwide"><!-- wp:column -->
<div class="wp-block-column"><!-- wp:paragraph {"style":{"typography":{"fontSize":24,"lineHeight":"1.17"}}} -->
<p style="font-size:24px;line-height:1.17;"><strong>Choose a plan</strong></p>
<!-- /wp:paragraph -->

<!-- wp:paragraph {"style":{"typography":{"fontSize":17,"lineHeight":"1.42"}}} -->
<p style="font-size:17px;line-height:1.42;">Whether you are finding yourself with more time in your hands that you’d have wanted or time is never to be found, we have a plan for you.</p>
<!-- /wp:paragraph -->

<!-- wp:paragraph {"style":{"typography":{"fontSize":17,"lineHeight":"1.42"}}} -->
<p style="font-size:17px;line-height:1.42;"><a href="#"><strong>Learn more about our plans</strong></a></p>
<!-- /wp:paragraph --></div>
<!-- /wp:column -->

<!-- wp:column -->
<div class="wp-block-column"><!-- wp:paragraph {"style":{"typography":{"fontSize":24,"lineHeight":"1.17"}}} -->
<p style="font-size:24px;line-height:1.17;"><strong>Easy ingredients</strong></p>
<!-- /wp:paragraph -->

<!-- wp:paragraph {"style":{"typography":{"fontSize":17,"lineHeight":"1.42"}}} -->
<p style="font-size:17px;line-height:1.42;">We like finding excellence in simple things and bring the best of simple, easy to find ingredients by working with their best combinations.</p>
<!-- /wp:paragraph -->

<!-- wp:paragraph {"style":{"typography":{"fontSize":17,"lineHeight":"1.42"}}} -->
<p style="font-size:17px;line-height:1.42;"><a href="#"><strong>Checkout sample ingredients</strong></a></p>
<!-- /wp:paragraph --></div>
<!-- /wp:column -->

<!-- wp:column -->
<div class="wp-block-column"><!-- wp:paragraph {"style":{"typography":{"fontSize":24,"lineHeight":"1.17"}}} -->
<p style="font-size:24px;line-height:1.17;"><strong>A dish to impress</strong></p>
<!-- /wp:paragraph -->

<!-- wp:paragraph {"style":{"typography":{"fontSize":17,"lineHeight":"1.42"}}} -->
<p style="font-size:17px;line-height:1.42;">You will not only learn new skills but you will also be able to share and impress you loved ones with your creations.</p>
<!-- /wp:paragraph -->

<!-- wp:paragraph {"style":{"typography":{"fontSize":17,"lineHeight":"1.42"}}} -->
<p style="font-size:17px;line-height:1.42;"><a href="#"><strong>See our past dishes</strong></a></p>
<!-- /wp:paragraph --></div>
<!-- /wp:column --></div>
<!-- /wp:columns -->

<!-- wp:spacer {"height":64} -->
<div style="height:64px;" aria-hidden="true" class="wp-block-spacer"></div>
